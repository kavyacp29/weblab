<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    <script>
        // Create an empty Set
        var mySet = new Set();

        // Add values to the Set
        mySet.add(1);
        mySet.add(2);
        mySet.add(3);
        mySet.add(4);
        mySet.add(5);

        // Retrieve a value from the Set
        var value = 3;
        if (mySet.has(value)) {
            console.log("Value", value, "is present in the Set");
        } else {
            console.log("Value", value, "is not present in the Set");
        }

        // Iterate over the Set
        console.log("Set values:");
        mySet.forEach(function (item) {
            console.log(item);
        });

    </script>
</body>
</html>
